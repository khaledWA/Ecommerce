         as=\"select\"\n                                                                value={qty}\n                                                                onChange={(e) => setQty(e.target.value)}\n                                                            >\n                                                                {\n\n                                                                    [...Array(product.countInStock).keys()].map((x) => (\n                                                                        <option key={x + 1} value={x + 1}>\n                                                                            {x + 1}\n                                                                        </option>\n                                                                    ))\n                                                                }\n\n                                                            </Form.Control>\n                                                        </Col>\n                                                    </Row>\n                                </ListGroup.Item>\n                            )}\n\n\n                            <ListGroup.Item>\n                                <Button \n                                onClick={addToCartHandler}\n                                className='btn-block' \n                                disabled={product.countInStock === 0} \n                                type='button'>Add to Cart\n                                </Button>\n                            </ListGroup.Item>\n\n                        </ListGroup>\n                    </Card>\n                </Col>\n            </Row>\n                )\n\n\n            }\n\n            \n        </div>\n    )\n}\n\nexport default ProductScreen\n","/Users/ahmadsawan/Desktop/ecommerce/frontend/src/components/Product.js",[],"/Users/ahmadsawan/Desktop/ecommerce/frontend/src/components/Rating.js",[],"/Users/ahmadsawan/Desktop/ecom